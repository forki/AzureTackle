{
    "private": true,
    "scripts": {
        "postcss:watch:docs": "postcss ./src/Docs/styles/additional_styles.css -o ./src/Docs/styles/output.css --env development -w",
        "postcss:build:docs": "postcss ./src/Docs/styles/additional_styles.css -o ./src/Docs/styles/output.css --env production",
        "vite:serve:docs": "vite serve ./src/Docs --port 5000",
        "vite:build:docs": "vite build ./src/Docs",
        "prestart": "dotnet tool restore",
        "startdocs": "cross-os startdocs"
    },
    "cross-os": {
        "startdocs": {
            "darwin": "NODE_ENV=development dotnet fable watch ./src/Docs --outDir src/Docs/output --run npm-run-all -s -p postcss:watch:docs vite:serve:docs",
            "linux": "NODE_ENV=development dotnet fable watch ./src/Docs --outDir src/Docs/output --run parcel serve ./src/Docs/index.html" ,
            "win32": "set NODE_ENV=development& dotnet fable watch ./src/Docs --outDir src/Docs/output --run parcel serve"
        }
    },
    "devDependencies": {
        "@babel/core": "^7.6.4",
        "@babel/plugin-transform-runtime": "^7.6.2",
        "@babel/preset-env": "^7.9.6",
        "@babel/runtime": "^7.6.3",
        "autoprefixer": "10.4.7",
        "babel-loader": "^8.0.5",
        "buffer": "^6.0.3",
        "core-js": "^3.3.2",
        "cross-os": "^1.4.0",
        "css-loader": "^4.2.1",
        "cssnano": "5.1.11",
        "file-loader": "^6.2.0",
        "mini-css-extract-plugin": "^0.10.0",
        "npm-run-all": "^4.1.5",
        "vite": "^5.0.13",
        "postcss-cli": "^9.1.0",
        "postcss-fontpath": "^1.0.0",
        "postcss-import": "^14.0.2",
        "postcss-loader": "^6.2.0",
        "process": "^0.11.10",
        "querystring-es3": "^0.2.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "remotedev": "^0.2.9",
        "resolve-url-loader": "^3.1.2",
        "sass": "^1.26.10",
        "sass-loader": "^9.0.3",
        "source-map-loader": "^3.0.0",
        "selfsigned": "^1.10.8",
        "style-loader": "^1.2.1"  },
    "dependencies": {
        "daisyui": "^3.0.0",
        "hammerjs": "^2.0.8",
        "tailwindcss": "^3.0.24"
    }
}
